{
  "expo": {
    "name": "Task Manager",
    "slug": "task-manager-field",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1a73e8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taskmanager.field"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1a73e8"
      },
      "package": "com.taskmanager.field"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}